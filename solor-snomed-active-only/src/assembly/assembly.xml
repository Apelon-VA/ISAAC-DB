<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<assembly xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2 http://maven.apache.org/xsd/assembly-1.1.2.xsd">
	<id>${resultArtifactClassifier}.bdb</id>
	<formats>
		<format>zip</format>
	</formats>
	<baseDirectory>${artifactId}-${version}${resultArtifactClassifierWithLeadingHyphen}.bdb</baseDirectory>
	<fileSets>
		<fileSet>
			<outputDirectory>/</outputDirectory>
			<directory>${project.build.directory}/${project.build.finalName}${resultArtifactClassifierWithLeadingHyphen}.bdb/</directory>
		</fileSet>
	</fileSets>
</assembly>
